# SkyMentor Environment Configuration
# Copy this file to .env and update with your actual values

# Database Configuration (PostgreSQL)
# Full connection string for Neon/other hosted PostgreSQL
DATABASE_URL=postgresql://username:password@host:port/database

# Individual database connection parameters
# (Used by Python setup script and other tools)
PGHOST=localhost
PGPORT=5432
PGDATABASE=skymentor
PGUSER=your_username
PGPASSWORD=your_password

# Application Configuration
NODE_ENV=development

# Session Configuration
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# Optional: External Service Keys
# Add any API keys or external service credentials here
# STRIPE_SECRET_KEY=sk_test_...
# TWILIO_ACCOUNT_SID=AC...
# TWILIO_AUTH_TOKEN=...